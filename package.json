{
  "name": "last-react-app",
  "version": "1.0.0",
  "description": "test web application",
  "main": "server.js",
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.4"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "remotedev": "remotedev --hostname=localhost --port=5000",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm install terser@3.14.1 --prefix client && npm run build --prefix client"
  },
  "author": "Apakorn",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "grpc": "^1.24.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-pre-gyp": "^0.14.0",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-server": "^0.3.1",
    "aws-sdk": "2.x"
  },
  "binary": {
    "module_name": "node_addon_example",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://node-pre-gyp-tests.s3-us-west-1.amazonaws.com"
  }
}
