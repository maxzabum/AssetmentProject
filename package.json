    "cors": "^2.8.5",
    "create-react-app": "^3.3.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^1.5.1",
    "mongoose": "^5.8.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "qrcode.react": "^1.0.0",
    "react-hook-form": "^4.6.2",
    "react-redux": "^7.1.3",
